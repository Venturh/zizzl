		"@tailwindcss/typography": "^0.4.1",
		"@types/react": "17.0.14",
		"autoprefixer": "^10.3.1",
		"eslint": "7.31.0",
		"eslint-config-next": "11.0.1",
		"postcss": "^8.3.6",
